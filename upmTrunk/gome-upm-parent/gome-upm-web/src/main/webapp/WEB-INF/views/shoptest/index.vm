    <!-- Bootstrap 3.3.4 -->
    <link href="${rc.contextPath}/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!-- FontAwesome 4.3.0 -->
    <link href="${rc.contextPath}/libs/font-awesome-4.4.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <!-- Ionicons 2.0.0 -->

    <!-- Theme style -->
    <link href="${rc.contextPath}/libs/css/AdminLTE.min.css" rel="stylesheet" type="text/css" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="${rc.contextPath}/libs/css/skins/_all-skins.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="${rc.contextPath}/css/base.css" type="text/css"/>
    <link rel="stylesheet" href="${rc.contextPath}/css/style.css" type="text/css"/>
    <link rel="stylesheet" href="${rc.contextPath}/css/common-top.css" type="text/css" />
    
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="${rc.contextPath}/libs/js/html5shiv.js" type="text/javascript"></script>
    <script src="${rc.contextPath}/libs/js/respond.js" type="text/javascript"></script>
    <![endif]-->
</head>
<body class="sidebar-mini skin-black-light">
<div class="wrapper">

    <!-- header start-->
    #parse("common/header.vm")
    <!-- header end-->

    <!-- Left side column. contains the logo and sidebar -->
    #parse("common/leftMenu.vm")
    <!-- Left side end -->

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <a href="${rc.contextPath}/index">统一监控平台 </a><a href="javascript:void(0)">/ 业务监控</a><a href="${rc.contextPath}/asp/index">/ 自动化购物测试</a>
        </section>

        <!-- Main content -->
        <section class="content">
        	<div class="health_index_facility">
       			<h2>下单主流程</h2>
       			<div style="height: 250px;margin: auto;" id="network_health_index"></div>
				<div class="health_status_count_server">
	        		<div class="border_R error_status">
	        		     <p>失败</p>
		       		     <p><a href="${rc.contextPath}/asp/list/normal/fail" id="errorDeviceCount">0</a></p>
		      	       </div>
	        		<div class="all_status">
	        		   <p>通过</p> 
	        		   <p><a href="${rc.contextPath}/asp/list/normal/pass" id="totalDeviceCount">0</a></p>
	        		</div>
	        	</div>
       		</div>
       		<div class="health_index_facility">
       			<h2>搜索下单主流程</h2>
       			<div style="height: 250px;margin: auto;" id="port_health_index"></div>
				<div class="health_status_count_server  ">
					<div class="border_R error_status">
						<p>失败</p>
						<p><a href="${rc.contextPath}/asp/list/search/fail" id="errorSensorCount">0</a></p>
						
					</div>
				    <div class="border_R all_status">
				        <p>通过</p>
				    	<p><a href="${rc.contextPath}/asp/list/search/pass" id="totalSensorCount">0</a></p>
				    	
				    </div>
				</div>
       		</div>

     		<div style="width: 66%;height:1000px;">
     		    <p id="time_use"></p>
               <!-- <p id="memory_use"></p>
                <p id="port_flow"></p>-->
            </div>   

			<div class="pie_item_service">
                <p id="cpu_use"></p>
                <p id="memory_use"></p>
                <!--<p id="port_flow"></p>-->
            </div>     
            
     
        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->
    <!-- footer start -->
      #parse("common/footer.vm")
    <!-- footer end -->
</div><!-- ./wrapper -->

<!-- jQuery 2.1.3 -->
<script src="${rc.contextPath}/js/jQuery/jQuery-2.1.3.min.js"></script>
<!-- Bootstrap 3.3.2 JS -->
<script src="${rc.contextPath}/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<!-- AdminLTE App -->
<script src="${rc.contextPath}/libs/js/app.min.js" type="text/javascript"></script>
<script src="${rc.contextPath}/js/left_menu.js" type="text/javascript"></script>
<script src="${rc.contextPath}/js/highcharts.js" type="text/javascript"></script>
<script src="${rc.contextPath}/js/highcharts-more.js" type="text/javascript"></script>
<script src="${rc.contextPath}/js/solid-gauge.js" type="text/javascript"></script>
<!--<script src="${rc.contextPath}/js/prtg/networkMonitor.js" type="text/javascript"></script>-->
<script src="${rc.contextPath}/js/shoptest/index.js" type="text/javascript"></script>
</body>
</html>
