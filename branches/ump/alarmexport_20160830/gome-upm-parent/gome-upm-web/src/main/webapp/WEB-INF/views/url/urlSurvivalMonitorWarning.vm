<body class="sidebar-mini skin-black-light">
<div class="wrapper">
    <!-- header start-->
    #parse("common/header.vm")
    <!-- header end-->
    
    <!-- Left side column. contains the logo and sidebar -->
    #parse("common/leftMenu.vm")
    <!-- Left side end -->
    
    
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <a href="${rc.contextPath}/index">统一监控平台 </a><a href="${rc.contextPath}/url/get">/ URL存活监控</a><a href="${rc.contextPath}/url/warning"> / 报警记录</a>
        	<a href="${rc.contextPath}/url/get" class="newbtn pull-right btnBack">返回</a>
        </section>
		<input type="hidden" id="start"/>
		<input type="hidden" id="end"/>
		<input type="hidden" id="search"/>
        <!-- Main content -->
        <section class="content contentSty">
        	
                    <div class="toolbar">
		                <form class="form-inline tool_select" >
							<div class="form-group">
								<label for="startTime">报警时间：</label> <input type="text" value="$!start"
									class="form-control laydate-icon" id="startTime" onclick="laydate({istime: true, format: 'YYYY-MM-DD hh:mm:ss'})">
							</div>
							<div class="form-group">
								<label for="endTime">至</label> <input type="text" value="$!end"
									class="form-control laydate-icon" id="endTime" onclick="laydate({istime: true,format: 'YYYY-MM-DD hh:mm:ss'})">
							</div>
							<div class="form-group">
								<label for="startTime">监控点地址：</label> <input type="text" value="$!search"
									class="form-control" id="urlAddress">
							</div>
							<!--<div class="form-group" style="margin-right: 20px;">
							  	<label>处理状态:</label>
							    <select class="form-control"> 
							      	<option>全部</option>
							      	<option>已处理</option>
							      	<option>忽略</option>
							    </select>
						  	</div>-->
							<a id="search_id" class="newbtn search_btn ReactiveURLbtn">
								<i class="fa fa-search"></i> 搜索
							</a>
						</form>
		            </div><!--toolbar end -->

                <div class="list_table">
 
                         #parse("url/urlWarningTable.vm")
                 </div>

        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->
    
     <!-- footer start -->
      #parse("common/footer.vm")
    <!-- footer end -->


    <!-- Control Sidebar -->
      #parse("common/controlSidebar.vm")
    <!-- Control Sidebar end-->
    
</div><!-- ./wrapper -->

<!-- jQuery 2.1.3 -->
<script src="${rc.contextPath}/js/jQuery/jQuery-2.1.3.min.js"></script>
<!-- Bootstrap 3.3.2 JS -->
<script src="${rc.contextPath}/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<!-- AdminLTE App -->
<script src="${rc.contextPath}/libs/js/app.min.js" type="text/javascript"></script>
<script src="${rc.contextPath}/js/left_menu.js" type="text/javascript"></script>
<script src="${rc.contextPath}/js/url/urlWarning.js" type="text/javascript"></script>
<script src="${rc.contextPath}/js/plugins/laydate-v1.1/laydate/laydate.js"></script>
