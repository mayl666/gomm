<table class="table table-striped table_data">
    <tbody>
		<tr>
		  <th>日期时间</th>
		  <th>值</th>
		  <th>停机时间</th>
		  <th>覆盖率</th>
		</tr>
        #if($!page.totalResult > 0)
    	#set($i = ($page.pageNo - 1) * $page.pageSize)
