		<tr>
		  <td>$!historyData.datetime</td>
		  <td>$!historyData.cpuMemVal</td>
		  <td>$!historyData.halt_time</td>
		  <td>$!historyData.coverage</td>
		</tr>
		#end
		#end
	</tbody>
</table>
#parse("network/pagePlugin.vm")
